<Window x:Class="Password_Manager.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Password_Manager"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="200" Width="400">
    <Grid>
        <TextBox HorizontalAlignment="Left" Name="RegEmail" Height="20" Margin="200,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox HorizontalAlignment="Left" Name="RegUsername" Height="20" Margin="200,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <PasswordBox HorizontalAlignment="Left" Name="RegPassword" Margin="200,100,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Email:" HorizontalAlignment="Right" Margin="0,20,210,0" VerticalAlignment="Top"/>
        <Label Content="Username:" HorizontalAlignment="Right" Margin="0,60,210,0" VerticalAlignment="Top"/>
        <Label Content="Password:" HorizontalAlignment="Right" Margin="0,100,210,0" VerticalAlignment="Top"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="160,140,0,0" VerticalAlignment="Top" Width="80"/>

    </Grid>
</Window>
